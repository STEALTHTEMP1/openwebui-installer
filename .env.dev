# Development Environment Configuration
COMPOSE_PROJECT_NAME=openwebui-installer-dev
PYTHONPATH=/workspace
PYTHONDONTWRITEBYTECODE=1
PYTHONUNBUFFERED=1
DEVELOPMENT=true

# Ports
DEV_PORT=8000
JUPYTER_PORT=8888
DOCS_PORT=8080
MONITOR_PORT=3000

# Database
POSTGRES_DB=openwebui_test
POSTGRES_USER=testuser
POSTGRES_PASSWORD=testpass

# API Keys (set these with your actual keys)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
HUGGINGFACE_TOKEN=

# Docker
DOCKER_BUILDKIT=1
COMPOSE_DOCKER_CLI_BUILD=1

# Development settings
DEBUG=true
LOG_LEVEL=DEBUG
VERBOSE=true

# Testing
TEST_DATABASE_URL=postgresql://testuser:testpass@test-db:5432/openwebui_test
REDIS_URL=redis://redis:6379/0

# Security (development only)
SECRET_KEY=dev-secret-key-do-not-use-in-production
DISABLE_AUTH=false

# Features
ENABLE_JUPYTER=true
ENABLE_AI_TOOLS=false
ENABLE_MONITORING=false
ENABLE_DOCS=false
